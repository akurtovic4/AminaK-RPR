<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="244.0" prefWidth="377.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" >

   <stylesheets>
      <URL value="@/css/login.css" />
   </stylesheets>
   <children>
      <ImageView fitHeight="267.0" fitWidth="406.0">
         <image>
            <Image url="@../img/pocetna.jpeg" />
         </image>
      </ImageView>
      <TextField layoutX="33.0" layoutY="67.0" promptText="Username" />
      <PasswordField layoutX="33.0" layoutY="108.0" promptText="Password" />
      <ButtonBar layoutX="33.0" layoutY="199.0" prefHeight="45.0" prefWidth="145.0">
         <buttons>
            <Button fx:id="btnOkey" mnemonicParsing="false" onAction="#showHomeUser" text="Ok">
               <padding>
                  <Insets right="5.0" />
               </padding>
            </Button>
            <Button fx:id="btnLoginBack" mnemonicParsing="false" onAction="#backToHome" text="Back">
               <padding>
                  <Insets right="5.0" />
               </padding>
            </Button>
         </buttons>
      </ButtonBar>
      <Text fill="#36258a" layoutX="33.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="LOG IN">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
